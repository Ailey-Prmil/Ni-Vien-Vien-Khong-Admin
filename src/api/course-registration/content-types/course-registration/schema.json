{
  "kind": "collectionType",
  "collectionName": "course_registrations",
  "info": {
    "singularName": "course-registration",
    "pluralName": "course-registrations",
    "displayName": "CourseRegistration"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "fullName": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true
    },
    "phoneNumber": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true,
      "regex": "^\\+?[0-9]\\d{7,14}$"
    },
    "emaiil": {
      "type": "email",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true
    },
    "address": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "confirmed": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "registedCourse": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::course.course",
      "inversedBy": "courseRegistration"
    }
  }
}
